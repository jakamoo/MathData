{
    "inherits" : ["Graph"],
    "name": "Edge Weighted Graph",
    "parameters" :
      {
        "edge_weight_type" : {
                      			"value": "Number",
                      			"default": "Integer"
                    		}
      },
    "raw" :
      {
        "dense":
          {
            "edges":
            {
              "element": {
              			"value": "{{edge_weight_type}}"
              			}
            }
          },
        "sparse":
          {
            "edges":
              {
                "element":
                   {

							"structure" : {
													"type": "Tensor",
													"size": ["Integer", "3"],
													"dimension": "2"
								},
									"element":  {
												"value": ["Integer", "Integer", "{{edge_weight_type}}"]
									}
								
                   }
              }
          }
     }
}
