{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {

        // Development settings
        "member-access": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-unused-variable": false,
        "no-unused-expression": false
    },
    "rulesDirectory": []
}