{
    "inherits" : ["Graph"],
    "name": "Edge Weighted Graph",
    "parameters" :
      {
        "edge_weight_type" : {
                      				  "value": "Number,
                      				  "default": "Integer"
                    			    }
      },
    "raw" :
      {
        "dense":
          {
            "edges":
            {
              "element": {
              							"value": "{{edge_weight_type}}"
              					 }
            }
          },
        "sparse":
          {
            "edges":
              {
                "element":
                   {
			              "structure" : {
                    								"type": "Tensor",
                    								"size": "Integer",
                    								"dimension": 1
                    							},
							"element":
								{
									"structure" : {
											"type": "Tensor",
											"size": 3,
											"dimension": 1
									},
									"element":  {
											"value": ["Integer", "{{edge_weight_type}}"],
											"default": ["1", "Integer"]
									}
								}
                   }
              }
          }
     }
}
